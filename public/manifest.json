{
  "manifest_version": 2,
  "name": "{{envBadge}}Vigvam â€” Web 3.0 Wallet",
  "version": "{{pkg.version}}",

  "icons": {
    "16": "icons/16.png",
    "24": "icons/24.png",
    "32": "icons/32.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },

  "description": "__MSG_appDesc__",
  "homepage_url": "{{website}}",
  "short_name": "Vigvam",

  "permissions": [],
  "content_security_policy": "script-src 'self'; object-src 'self'",

