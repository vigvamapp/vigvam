      "16": "icons/16.png",
      "24": "icons/24.png",
      "32": "icons/32.png",
      "48": "icons/48.png",
      "128": "icons/128.png"
    },
    "__chrome__chrome_style": false,
    "__firefox__browser_style": false
  },

  "background": {
    "page": "back.html",
    "persistent": true
  },

  "content_scripts": [],

  "web_accessible_resources": []
